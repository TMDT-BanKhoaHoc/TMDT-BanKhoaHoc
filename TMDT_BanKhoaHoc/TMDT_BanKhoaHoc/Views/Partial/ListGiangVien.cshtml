@model IEnumerable<TMDT_BanKhoaHoc.Models.GIANGVIEN>
@{ 
    Layout = null;
}
@if (Model != null)
{
    foreach (var item in Model)
    {
        <li class="list-item">
            <a href="@Url.Action(controllerName: "NguoiDung", actionName: "KhoaHocTheoGiangVien", routeValues: new { giangvien = item.MaGV })">
                @item.TenGV
            </a>
        </li>
    }
}